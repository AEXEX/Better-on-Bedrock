						},
						{
							"active2": "query.variant == 2"
						}
					]
				},
				"active2": {
					"animations": [
						"attack2"
					],
					"transitions": [
						{
							"default": "query.variant == 1"
						},
						{
							"active": "query.variant == 0"
						}
					]
				}
			}
		}
	}
